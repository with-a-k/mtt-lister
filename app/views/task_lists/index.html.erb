<div class="row">
	<div class="small-12 columns">
		<h2>Your Task Lists</h2>
	</div>
</div>

<div class="row">
	<div class="small-12 columns">
		<ul class="task-lists">
			<% @task_lists.each do |task_list| %>
				<li>
					<h3><%= link_to task_list.title, user_task_list_path(current_user, task_list) %></h3>
				</li>
			<% end %>
		</ul>
	</div>
</div>