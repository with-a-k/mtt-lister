<div class="row">
	<div class="small-12 columns">
		<%= form_for @task_list, url: user_task_list_path(current_user, @task_list) do |f| %>
			<%= f.label(:title, "Title") %>
			<%= f.text_field(:title) %>
			<% f.hidden_field :user_id, value: current_user.id %>
			<%= submit_tag("Edit") %>
		<% end %>
	</div>
</div>